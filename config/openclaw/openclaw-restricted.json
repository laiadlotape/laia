{
  "_comment": "LAIA OpenClaw Restricted Configuration â€” Default maximum security",
  "_warning": "DO NOT edit this file directly. Use 'laia-config' GUI to modify safely.",
  "_version": "1.0.0",

  "security": {
    "mode": "restricted",
    "bind": "127.0.0.1",
    "allowlist": {
      "enabled": true,
      "paths": [
        "~/.openclaw/workspace",
        "~/.openclaw/laia"
      ]
    },
    "exec": {
      "ask": "always",
      "security": "allowlist",
      "elevated": false
    },
    "browser": {
      "allowedProfiles": ["openclaw"],
      "blockChrome": true
    },
    "network": {
      "allowedDomains": [
        "api.anthropic.com",
        "api.openai.com",
        "localhost",
        "127.0.0.1"
      ],
      "blockExternalRequests": false
    }
  },

  "workspace": {
    "path": "~/.openclaw/workspace",
    "autoCommit": false,
    "gitPush": false
  },

  "channels": {},

  "features": {
    "nodes": false,
    "camera": false,
    "location": false,
    "notifications": "none"
  },

  "rateLimit": {
    "requestsPerMinute": 20,
    "dailyBudget": 100
  }
}
